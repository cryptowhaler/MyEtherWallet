{
  "name": "myetherwallet",
  "version": "5.0.0",
  "description": "Client side ethereum wallet",
  "scripts": {
    "dev": "vue-cli-service serve",
    "start": "npm run update:lists && npm run dev",
    "test:unit": "vue-cli-service test:unit",
    "test": "npm run test:unit",
    "lint:styles": "prettier --write 'src/**/*.scss' && stylelint 'src/**/*.scss' --fix",
    "lint:code": "vue-cli-service lint --fix",
    "lint": "npm run lint:styles && npm run lint:code",
    "build": "npm run update:lists && vue-cli-service build",
    "deploy": "gh-pages -d dist -b gh-pages",
    "codecov": "npm run test:unit && codecov",
    "update:lists": "node fetchEthListData.js"
  },
  "gitHooks": {
    "pre-commit": "npm run lint && npm run test"
  },
  "dependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.5.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-vue": "^6.2.2",
    "vue": "^2.6.11",
    "vue-router": "^3.3.4",
    "vuex": "^3.5.1",
    "websocket": "^1.0.31"
  },
  "devDependencies": {
    "@ledgerhq/hw-app-eth": "^5.20.0",
    "@ledgerhq/hw-transport-u2f": "^5.19.1",
    "@myetherwallet/mewconnect-web-client": "2.1.1-RC.0",
    "@vue/cli-plugin-babel": "^4.4.6",
    "@vue/cli-plugin-eslint": "^4.4.6",
    "@vue/cli-plugin-unit-jest": "^4.4.6",
    "@vue/cli-service": "^4.4.6",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/test-utils": "^1.0.3",
    "@xkeshi/vue-qrcode": "^1.0.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^26.1.0",
    "bignumber.js": "^9.0.0",
    "bip39": "^3.0.2",
    "bootstrap-vue": "^2.15.0",
    "canvas": "^2.6.1",
    "codecov": "^3.7.2",
    "dotenv-webpack": "^2.0.0",
    "ethereum-ens": "git+https://github.com/ensdomains/ensjs.git",
    "ethereumjs-tx": "^2.1.2",
    "ethereumjs-util": "^7.0.3",
    "ethereumjs-wallet": "^1.0.0",
    "ethjs-unit": "^0.1.6",
    "hdkey": "^2.0.1",
    "imagemin-webpack-plugin": "^2.4.2",
    "keccak": "^3.0.1",
    "node-fetch": "^2.6.0",
    "node-sass": "^4.14.1",
    "postcss-import": "^12.0.1",
    "postcss-url": "^8.0.0",
    "prettier": "^2.0.5",
    "sass-loader": "^9.0.2",
    "secp256k1": "^4.0.2",
    "store": "^2.0.12",
    "stylelint": "^13.6.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^20.0.0",
    "vue-i18n": "^8.19.0",
    "vue-infinite-slide-bar": "^1.1.1",
    "vue-template-compiler": "^2.6.11",
    "vue-worker": "^1.2.1",
    "web3": "^1.2.11",
    "worker-loader": "^2.0.0",
    "zxcvbn": "^4.4.2"
  },
